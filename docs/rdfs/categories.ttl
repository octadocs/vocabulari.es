@prefix : <local:> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Category rdfs:subClassOf rdfs:Class .

:color
  a rdf:Property ;
  rdfs:domain :Category ;
  rdfs:range xsd:string ;
  rdfs:label "Color of the card." .

:priority
  a rdf:Property ;
  rdfs:domain :Category ;
  rdfs:range xsd:integer ;
  rdfs:label "Order of categories on screen." .

:column_width
  a rdf:Property ;
  rdfs:domain :Category ;
  rdfs:range xsd:integer .

:Entities
  a :Category ;
  :color "red" ;
  :priority 0 ;
  rdfs:label "Classes" ;
  :column_width "sixteen" ;
  rdfs:comment "Well, you know... things." .


:Containers
  a :Category ;
  :color "purple" ;
  :priority 9 ;
  :column_width "sixteen" ;
  rdfs:label "Containers" ;
  rdfs:comment "Lists, sets, boxes, crates, bags, ..." .


:DomainAndRange
  a :Category ;
  :column_width "eight" ;
  :priority 1 ;
  rdfs:label "Domain & Range" .


:Sub
  a :Category ;
  :column_width "eight" ;
  :priority 4 ;
  rdfs:label "Sub" ;
  rdfs:comment "Properties linked together." .


:ResourceToResource
  a :Category ;
  :column_width "eight" ;
  :color "yellow" ;
  :priority 2 ;
  rdfs:comment "Links from one thing to another." ;
  rdfs:label "Resource â†’ Resource" .

:TextualDescriptions
  a :Category ;
  :column_width "eight" ;
  :color "green" ;
  :priority 3 ;
  rdfs:label "Textual Descriptions" ;
  rdfs:comment "Telling the humans what's going on." .
